@using Integratieproject1.Domain.Projects
@using Integratieproject1.UI.Controllers
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta name="google-site-verification" content="CIYO5SWRNrTqz7NScM_jxfBQMXuf4ESBGR7sCkrihhc" />
    <title>@ViewData["Title"] - City Of Ideas</title>
    <script type="module" src="~/dist/site.entry.js" defer></script>
    <link rel="stylesheet" href="~/dist/site.css">
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body>
<nav class="navbar navbar-expand-lg navbar-dark bg-secondary">
    <div class="container">
        
        <a class="navbar-brand" asp-controller="Home" asp-action="Index">CityOfIdeas</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <partial name="/UI/Views/Shared/_SearchPartial.cshtml"/>
            <partial name="/UI/Views/Shared/_LoginPartial.cshtml"/>
            <partial  name="/UI/Views/Shared/_SharedPartial.cshtml"/>
            <ul class="nav navbar-nav navbar-right">
               <li class="nav-item">
                               <a class="nav-link" asp-controller="Home" asp-action="Index"><i class="fa fa-home"></i></a>
                           </li> 
            </ul>
            
            
        </div>
    </div>
</nav>

<div class="body-content">
    @RenderBody()
    <footer >
        <p>&copy; 2019 - Integratieproject1 - <a asp-controller="Home" asp-action="About">Info</a> - <a asp-controller="Home" asp-action="FAQ">FAQ</a> - <a asp-controller="Home" asp-action="Privacy">Privacy</a></p>
    </footer>
</div>

@RenderSection("Scripts", required: false)
</body>
</html>

